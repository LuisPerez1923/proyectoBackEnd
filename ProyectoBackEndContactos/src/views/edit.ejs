<%- include('partials/_header') %>
    <div class="container">
        <div class="row">
            <!--Formulario-->
            <div class="col-md-6 offset-md-3">
                <div class="card">
                    <div class="card-body">
                        <form action="/edit/<%= contacto._id %>" method="POST">
                            <div class="form-group">
                                <input type="text" name="nombre" id=""  class="form-control" value="<%= contacto.nombre %>">
                            </div>
                            <div class="form-group">
                                <input type="number" name="numAdress" id=""  class="form-control" value="<%= contacto.numAdress %>">
                            </div>
                            <div class="form-group">
                                <input type="number" name="celNumber" id=""  class="form-control" value="<%= contacto.celNumber %>">
                            </div>
                            <div class="form-group">
                                <input type="text" name="relacion" id=""  class="form-control" value="<%= contacto.relacion %>">
                            </div>
                            <button type="submit" class="btn btn-primary btn-block">
                                Actualizar
                            </button>
                        </form>
                    </div>
                </div>
            </div>
            
       
        </div>
    </div>
<%- include('partials/_footer') %>